[program]
class ifTest : .sht.cons.program {

   override run(args : str[]) : void
   {
      ._print("===if");
      ._print("1)");
      doTest(1);
      ._print("2)");
      doTest(2);
      ._print("3)");
      doTest(3);
      ._print("4)");
      doTest(4);
   }

   private doTest(flag : int) : void
   {
      ._print("---");
      if(flag < 2)
         ._print("1");
      else if(flag < 3)
         ._print("2");
      else
         ._print("3-4");

      if(flag < 4)
         ._print("1-3");
      else
         ._print("4");
   }

}
